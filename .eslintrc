{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": "off",
        "indent": ["error", "tab"],
        "no-multiple-empty-lines": [
            "error",
            {
              "max": 1,
              "maxEOF": 1
            }
          ], 
          "max-len": [
            2,
            250
          ]              
    }
}